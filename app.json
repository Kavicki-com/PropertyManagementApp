{
  "expo": {
    "name": "llord",
    "slug": "llord",
    "scheme": "llord",
    "version": "1.0.0",
    "owner": "kavickicom",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fafafa"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kavicki.com.llord",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
